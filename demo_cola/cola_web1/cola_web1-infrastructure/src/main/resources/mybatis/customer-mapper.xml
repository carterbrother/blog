<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.authine.web.cola.repository.CustomerMapper">
    <select id="getById" resultMap="customerMap">
        select * from customer where customer_id = #{id}
    </select>

    <resultMap type="com.authine.web.cola.repository.CustomerDO" id="customerMap">
         <result property="customerId" column="customer_id"/>
         <result property="memberId" column="member_id"/>
         <result property="globalId" column="global_id"/>
         <result property="registeredCapital" column="registered_capital"/>
         <result property="companyName" column="company_name"/>
     </resultMap>
</mapper>
